        <subplugin name="WechatFavorite" showName="微信收藏">
            <param name="SortId" value="7" desc="此平台在您分享列表中的位置，整型，数值越大越靠后" showName="SortId" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="AppId" value="wx9e91b08387f4fc2c" desc="填写您在微信上注册到的AppId" showName="AppId" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="AppSecret" value="6bcc5b6298ee39f7e373ceeb2b11cab2" desc="填写您在微信上注册到的AppSecret" showName="AppSecret" bWriteInManifest="0" bWriteInClient="0"/>                        
        </subplugin>                             

    </subplugins>

    <params>
        <param name="AppKey" value="androidv1101" desc="ShareSDK的appkey" showName="AppKey" bWriteIntoManifest="0" bWriteIntoClient="0" />
    </params>
    
    <operations>
        <operation step="1" type="mergeManifest" from="SDKManifest.xml" to="AndroidManifest.xml" />
        <operation step="2" type="copyRes" from="assets" to="assets" />
        <operation step="3" type="copyRes" from="libs" to="lib" />
        <operation step="4" type="copyRes" from="res" to="res" />
        <operation step="5" type="script" from="script.py"/>
    </operations>
    
    <plugins>
        <plugin name="com.u8.sdk.ShareSDKShare" type="4" desc="用户分享接口"/>
    </plugins>

    <version>
        <versionCode>1</versionCode>
        <versionName>2.5.7</versionName>
    </version>    
    
</config>