            <param name="AppSecret" value="ae36f4ee3946e1cbb98d6965b0b2ff5c" desc = "填写您在腾讯微博上注册到的AppSecret" showName="AppSecret" bWriteInManifest="0" bWriteInClient="0"/>

            <param name="RedirectUrl" value="http://www.sharesdk.cn" desc = "填写您在腾讯微博上注册的RedirectUrl" showName="RedirectUrl" bWriteInManifest="0" bWriteInClient="0"/>                                             
        </subplugin>        

        <subplugin name="QQ" showName="QQ">
            <param name="SortId" value="3" desc="此平台在您分享列表中的位置，整型，数值越大越靠后" showName="SortId" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="AppId" value="100371282" desc="填写您在QQ上注册到的AppId" showName="AppId" bWriteInManifest="0" bWriteInClient="0"/>  
            <param name="AppKey" value="aed9b0303e3ed1e27bae87c33761161d" desc="填写您在QQ上注册到的AppKey" showName="AppKey" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="ShareByAppClient" value="true" desc="是否使用QQ客户端进行分享" showName="ShareByAppClient" bWriteInManifest="0" bWriteInClient="0"/>                            
        </subplugin>

        <subplugin name="QZone" showName="QQ空间">
            <param name="SortId" value="4" desc="此平台在您分享列表中的位置，整型，数值越大越靠后" showName="SortId" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="AppId" value="100371282" desc="填写您在QZONE上注册到的AppId" showName="AppId" bWriteInManifest="0" bWriteInClient="0"/>  
            <param name="AppKey" value="aed9b0303e3ed1e27bae87c33761161d" desc="填写您在QZONE上注册到的AppKey" showName="AppKey" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="ShareByAppClient" value="true" desc="是否使用微博客户端进行分享" showName="ShareByAppClient" bWriteInManifest="0" bWriteInClient="0"/>                            
