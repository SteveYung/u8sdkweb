        <subplugin name="WechatMoments" showName="微信朋友圈">
            <param name="SortId" value="6" desc="此平台在您分享列表中的位置，整型，数值越大越靠后" showName="SortId" bWriteInManifest="0" bWriteInClient="0"/>
