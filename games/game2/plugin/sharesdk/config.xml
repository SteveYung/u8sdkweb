<?xml version="1.0" encoding="UTF-8"?>
<config>
    
    <subplugins>

        <subplugin name="SinaWeibo" showName="新浪微博">
            <param name="SortId" value="1" desc = "此平台在您分享列表中的位置，整型，数值越大越靠后" showName="SortId" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="AppKey" value="568898243" desc = "填写您在新浪微博上注册到的AppKey" showName="AppKey" bWriteInManifest="0" bWriteInClient="0"/>  
            <param name="AppSecret" value="38a4f8204cc784f81f9f0daaf31e02e3" desc = "填写您在新浪微博上注册到的AppSecret" showName="AppSecret" bWriteInManifest="0" bWriteInClient="0"/>

            <param name="RedirectUrl" value="http://www.sharesdk.cn" desc = "填写您在新浪微博上注册的RedirectUrl" showName="RedirectUrl" bWriteInManifest="0" bWriteInClient="0"/> 
            <param name="ShareByAppClient" value="http://www.sharesdk.cn" desc = "是否使用微博客户端进行分享" showName="ShareByAppClient" bWriteInManifest="0" bWriteInClient="0"/>                                             
        </subplugin>

        <subplugin name="TencentWeibo" showName="腾讯微博">
            <param name="SortId" value="2" desc = "此平台在您分享列表中的位置，整型，数值越大越靠后" showName="SortId" bWriteInManifest="0" bWriteInClient="0"/>
