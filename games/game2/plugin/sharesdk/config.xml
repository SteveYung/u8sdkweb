        <subplugin name="Wechat" showName="微信">
            <param name="SortId" value="5" desc="此平台在您分享列表中的位置，整型，数值越大越靠后" showName="SortId" bWriteInManifest="0" bWriteInClient="0"/>
            <param name="AppId" value="wx9e91b08387f4fc2c" desc="填写您在微信上注册到的AppId" showName="AppId" bWriteInManifest="0" bWriteInClient="0"/>  
